doctype html
html(lang='en')
  head
    block title
      title pixel2html-test
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name='description' content='pixel2html-test - Scaffolded with Pixel2HTML Frontend Generator')
    meta(name='theme-color', content='#3e456c')
    link(rel='manifest' href='assets/head/manifest.json')
    link(rel="shortcut icon" href="assets/head/favico.ico")
    link(rel="icon" type="image/png" href="assets/head/favicon.png")
    if production
      link(rel='stylesheet', href='assets/css/vendor.min.css')
      link(rel='stylesheet', href='assets/css/main.min.css')
      link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.0.13/css/solid.css" integrity="sha384-Rw5qeepMFvJVEZdSo1nDQD5B6wX0m7c5Z/pLNvjkB14W6Yki1hKbSEQaX9ffUbWe')
      link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.0.13/css/fontawesome.css" integrity="sha384-GVa9GOgVQgOk+TNYXu7S/InPTfSDTtBalSgkgqQ7sCik56N9ztlkoTr2f/T44oKV')

    else
      link(rel='stylesheet', href='assets/css/vendor.css')
      link(rel='stylesheet', href='assets/css/main.css')
      link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.0.13/css/solid.css" integrity="sha384-Rw5qeepMFvJVEZdSo1nDQD5B6wX0m7c5Z/pLNvjkB14W6Yki1hKbSEQaX9ffUbWe')
      link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.0.13/css/fontawesome.css" integrity="sha384-GVa9GOgVQgOk+TNYXu7S/InPTfSDTtBalSgkgqQ7sCik56N9ztlkoTr2f/T44oKV')

    block head
      //- If extra head content is necessary per page, it can be added per screen

  body
    include includes/mixins
    include general/menu
    block content
      //- This gets replaced by content per page

    include general/footer
    block footer
      //- If extra footer content is necessary per page, it can be added per screen

    if production
      script(src='//code.jquery.com/jquery-3.3.1.min.js')
    script(src='assets/js/main.js',type="text/javascript")
    block scripts
      //- Also if more scripts are needed you can hook in here <3
